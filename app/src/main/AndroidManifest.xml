        <activity android:name=".SimRegInfo" />
        <activity android:name=".UserLoginActivity" />
        <activity android:name=".SimRegListViewActivity" />
        <activity android:name=".SimRegSignature" />
        <activity android:name=".SimRegOfflineDataActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>